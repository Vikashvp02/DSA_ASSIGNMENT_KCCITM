#include <bits/stdc++.h>
using namespace std;

int main(){
    int a;
    cin>>a;
    int arr[a][a];
    for(int i=0;i<a;i++){
        for(int j=0;j<a;j++){
            cin>>arr[i][j];
        }
    }
    int a1=0;
    for(int i=0;i<a;i++){
        for(int j=0;j<a;j++){
            if(i==j){
                a1+=arr[i][j];
            }
        }
    }
    int a2=0;
    for(int i=0;i<a;i++){
        for(int j=0;j<a;j++){
            if(i+j==a-1){
                a2+=arr[i][j];
            }
        }
    }
    int sum=abs(a1-a2);
    cout<<sum;
    return 0;
}
